# PostgreSQL connection string
PGDOCTOR_DSN=postgres://user:password@localhost:5432/dbname

# Limit for queries/tables/indexes (1-200, default: 10)
PGDOCTOR_LIMIT=10

# API endpoint for uploading snapshots
PGDOCTOR_ENDPOINT=https://api.example.com/ingest

# API token for authentication
PGDOCTOR_TOKEN=your-api-token
